<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Sem_atividade_dias__c</fullName>
    <externalId>false</externalId>
    <formula>(IF(ISBLANK(LastActivityDate), 999, today() - LastActivityDate))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Este campo representa há quanto tempo a conta está sem atendimento.</inlineHelpText>
    <label>Sem atividade há (dias)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
