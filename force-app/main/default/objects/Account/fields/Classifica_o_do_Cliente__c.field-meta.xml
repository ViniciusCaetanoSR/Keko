<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Classifica_o_do_Cliente__c</fullName>
    <externalId>false</externalId>
    <formula>IF((ABS(COF__c) &lt;= 0.20), &quot;Fidelizado&quot;,
IF((ABS(COF__c) &gt; 0.20) &amp;&amp;
